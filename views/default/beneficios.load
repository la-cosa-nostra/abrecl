<script>
    var action = '{{=action}}';
    $(document).ready(function(){
        var size = size_window();
        $("#page-content").css('min-height',(size.height-100).toString()+'px');
        $(".navigation").hide();
        $("#loader").hide();
        $("#direccion b").append($("<span/>").attr('id','brand-menu-action').html(' / Beneficios'));
    });
    window.onresize = function(event) {
        var size = size_window();
        $("#page-content").css('min-height',(size.height-100).toString()+'px');
    };
</script>
